<div class="container">
  <div class="jumbotron">
    <ul class="breadcrumb">
      <li><%= link_to "Home", root_path %></li>
      <li><%= link_to "Husky Breeders", husky_breeders_path %></li>
    </ul>
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Breeder Name:</strong>
      <%= @husky_breeder.name %>
    </p>
    <%= image_tag @husky_breeder.image.url(:medium) %>
    <p>
      <strong>Website:</strong>
      <%= @husky_breeder.website %>
    </p>

    <p>
      <strong><span class="glyphicon glyphicon-envelope"></span>  Email:</strong>
      <%= @husky_breeder.email %>
    </p>

    <p>
      <strong><span class="glyphicon glyphicon-earphone"></span> Phone:</strong>
      <%= @husky_breeder.phone %>
    </p>

    <p>
      <strong>About the Breeder:</strong>
      <%= @husky_breeder.description %>
    </p>

    <%= link_to 'Edit', edit_husky_breeder_path(@husky_breeder) %> |
    <%= link_to 'Back', husky_breeders_path %>
  </div>
</div>
